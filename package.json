{
  "name": "moms-recipes",
  "version": "1.0.0",
  "description": "AI-powered recipe management system with PDF parsing, URL import, and calorie estimation",
  "author": "Michael Buckingham",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/mbuckingham74/moms-recipes.git"
  },
  "keywords": [
    "recipes",
    "ai",
    "pdf-parsing",
    "react",
    "express",
    "mysql",
    "claude",
    "openai",
    "gemini"
  ],
  "main": "backend/src/server.js",
  "scripts": {
    "start": "node backend/src/server.js",
    "dev": "nodemon backend/src/server.js",
    "test": "jest --runInBand --verbose --coverage --forceExit --testTimeout=30000",
    "test:watch": "jest --runInBand --watch",
    "setup": "npm install && cd frontend && npm install",
    "docker:up": "docker compose up -d --build",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testMatch": [
      "**/backend/tests/**/*.test.js"
    ]
  },
  "type": "commonjs",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.70.1",
    "@google/generative-ai": "^0.21.0",
    "bcrypt": "^6.0.0",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csrf-csrf": "^4.0.3",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.11.5",
    "openai": "^4.76.0",
    "pdf-parse": "^2.4.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4"
  }
}
